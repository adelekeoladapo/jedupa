Student
- Category
- Additional details



VIEWS


CREATE VIEW vw_class AS SELECT tb_class.class_id AS class_id, tb_class.school_id AS school_id, tb_class.student_department_id AS student_department_id, tb_class.name AS name, tb_class.code AS code, tb_class_level.class_level_id AS level_id, tb_class_level.name AS level_name, tb_class.date_created AS date_created, tb_student_department.name AS department_name, tb_student_department.code AS department_code FROM tb_class INNER JOIN tb_student_department ON tb_class.student_department_id = tb_student_department.student_department_id JOIN tb_class_level ON tb_class_level.class_level_id = tb_class.class_level_id